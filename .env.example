# Example Environment Variables for EcoSnap AI
# Copy this to .env and fill in your API keys

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Gemini API (Required for product analysis and AI features)
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_MODEL=gemini-2.5-flash

# OpenFoodFacts API (Free - No key needed)
VITE_OPENFOODFACTS_URL=https://world.openfoodfacts.org

# Unsplash API for Images (Free tier - get key from https://unsplash.com/developers)
VITE_UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# Optional APIs
VITE_CARBON_INTERFACE_KEY=your_carbon_interface_key_here
VITE_RECYCLENATION_KEY=your_recyclenation_key_here

# App Configuration
VITE_APP_NAME=EcoSnap AI
VITE_APP_VERSION=2.0.0
VITE_APP_ENV=development

# Feature flags
VITE_ENABLE_AR_SCANNER=true
VITE_ENABLE_BULK_SCAN=true
VITE_ENABLE_CAMERA_UPLOAD=true
VITE_ENABLE_BARCODE_SCANNER=true

# Enhanced Product Enrichment Configuration
VITE_ENABLE_REAL_DATA=true
VITE_ENABLE_INTELLIGENT_IMAGES=true
VITE_ENABLE_AI_ALTERNATIVES=true
VITE_FALLBACK_IMAGES=true

# API Rate Limiting and Performance
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000
VITE_CACHE_DURATION=3600000
VITE_MAX_ALTERNATIVES=5
VITE_IMAGE_SEARCH_LIMIT=10

# Enhanced Features
VITE_ENABLE_CONFIDENCE_SCORING=true
VITE_ENABLE_SOURCE_TRACKING=true
VITE_ENABLE_ENHANCED_METADATA=true

# Build optimization
VITE_BUILD_OPTIMIZE=true
VITE_BUILD_MINIFY=true
VITE_ENABLE_ANALYTICS=true

# Performance optimizations
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE=true
VITE_API_TIMEOUT=30000
VITE_MAX_FILE_SIZE=10485760